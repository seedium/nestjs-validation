{
  "name": "nestjs-validation",
  "version": "1.0.3",
  "description": "Validation module for NestJS application",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seedium/nestjs-validation.git"
  },
  "keywords": [
    "nestjs",
    "typescript",
    "seedium"
  ],
  "author": "Bogdan Sklyarov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seedium/nestjs-validation/issues"
  },
  "homepage": "https://github.com/seedium/nestjs-validation#readme",
  "scripts": {
    "clean": "rimraf -rf 'dist' 'tsconfig.tsbuildinfo'",
    "build": "yarn clean && tsc -p tsconfig.json",
    "watch": "yarn clean && tsc -w",
    "lint": "eslint lib/**/*.ts",
    "lint:test": "eslint -c .eslintrc.spec.yaml test/**/*.spec.ts",
    "format": "prettier '**/*.ts' --write",
    "test": "yarn clean && nyc --nycrc-path .nycrc.yaml mocha --reporter spec --exit 'test/**/*.spec.ts' 'test/*.spec.ts'",
    "test:integration": "yarn clean && nyc mocha 'integration/**/*.spec.ts' --reporter spec --exit",
    "prerelease": "yarn clean && yarn build",
    "release": "release-it"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-angular": "12.0.1",
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@nestjs/common": "7.6.15",
    "@nestjs/core": "7.6.15",
    "@nestjs/platform-express": "7.6.15",
    "@nestjs/swagger": "4.8.0",
    "@nestjs/testing": "7.6.15",
    "@release-it/conventional-changelog": "2.0.1",
    "@types/chai": "4.2.15",
    "@types/mocha": "8.2.2",
    "@types/node": "14.14.35",
    "@types/sinon": "9.0.11",
    "@types/sinon-chai": "3.2.5",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "chai": "4.3.4",
    "conventional-changelog-angular": "5.0.12",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "4.3.8",
    "light-my-request": "4.4.1",
    "lint-staged": "10.5.4",
    "mocha": "8.3.2",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "reflect-metadata": "0.1.13",
    "release-it": "14.5.0",
    "rimraf": "3.0.2",
    "rxjs": "6.6.6",
    "semver": "7.3.5",
    "sinon": "10.0.0",
    "sinon-chai": "3.6.0",
    "source-map-support": "0.5.19",
    "supertest": "6.0.1",
    "ts-node": "9.1.1",
    "ts-toolbelt": "8.0.7",
    "tslib": "2.0.3",
    "typescript": "4.0.5"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0 || ^7.0.0",
    "@nestjs/core": "^6.0.0 || ^7.0.0",
    "@nestjs/swagger": "^4.7.15",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0"
  },
  "dependencies": {
    "ajv": "7.2.3",
    "fluent-json-schema": "2.0.4",
    "lodash.pickby": "4.6.0"
  }
}
