{
  "name": "nestjs-validation",
  "version": "4.0.1",
  "description": "Validation module for NestJS application",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seedium/nestjs-validation.git"
  },
  "keywords": [
    "nestjs",
    "typescript",
    "seedium"
  ],
  "author": "Seedium <info@seedium.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seedium/nestjs-validation/issues"
  },
  "homepage": "https://github.com/seedium/nestjs-validation#readme",
  "scripts": {
    "clean": "rimraf -rf 'dist' 'tsconfig.tsbuildinfo'",
    "build": "yarn clean && tsc -p tsconfig.json",
    "watch": "yarn clean && tsc -w",
    "lint": "eslint lib/**/*.ts",
    "lint:test": "eslint -c .eslintrc.spec.yaml test/**/*.spec.ts",
    "format": "prettier '**/*.ts' --write",
    "test": "yarn clean && nyc --nycrc-path .nycrc.yaml mocha --reporter spec --exit 'test/**/*.spec.ts' 'test/*.spec.ts'",
    "test:integration": "yarn clean && nyc mocha 'integration/**/*.spec.ts' --reporter spec --exit",
    "prerelease": "yarn clean && yarn build",
    "release": "yarn prerelease && release-it",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-angular": "17.6.6",
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@nestjs/common": "10.0.5",
    "@nestjs/core": "10.0.5",
    "@nestjs/platform-express": "10.0.5",
    "@nestjs/swagger": "7.1.1",
    "@nestjs/testing": "10.0.5",
    "@release-it/conventional-changelog": "7.0.0",
    "@types/chai": "4.3.5",
    "@types/mocha": "10.0.1",
    "@types/node": "16.18.38",
    "@types/sinon": "10.0.15",
    "@types/sinon-chai": "3.2.9",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "ajv": "8.12.0",
    "chai": "4.3.7",
    "conventional-changelog-angular": "6.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "8.8.0",
    "husky": "8.0.3",
    "light-my-request": "5.10.0",
    "lint-staged": "13.2.3",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "pinst": "3.0.0",
    "prettier": "3.0.0",
    "reflect-metadata": "0.1.13",
    "release-it": "16.1.0",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1",
    "semver": "7.5.4",
    "sinon": "13.0.2",
    "sinon-chai": "3.7.0",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-node": "10.9.1",
    "ts-toolbelt": "9.6.0",
    "tslib": "2.6.0",
    "typescript": "5.1.6"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "@nestjs/core": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "@nestjs/swagger": "^4.7.15 || ^5.0.0",
    "ajv": "^8.6.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0 || ^7.0.0"
  },
  "dependencies": {
    "fluent-json-schema": "4.1.0",
    "lodash.pickby": "4.6.0"
  },
  "packageManager": "yarn@3.6.1"
}
