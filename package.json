{
  "name": "nestjs-validation",
  "version": "2.0.3",
  "description": "Validation module for NestJS application",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seedium/nestjs-validation.git"
  },
  "keywords": [
    "nestjs",
    "typescript",
    "seedium"
  ],
  "author": "Seedium <info@seedium.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seedium/nestjs-validation/issues"
  },
  "homepage": "https://github.com/seedium/nestjs-validation#readme",
  "scripts": {
    "clean": "rimraf -rf 'dist' 'tsconfig.tsbuildinfo'",
    "build": "yarn clean && tsc -p tsconfig.json",
    "watch": "yarn clean && tsc -w",
    "lint": "eslint lib/**/*.ts",
    "lint:test": "eslint -c .eslintrc.spec.yaml test/**/*.spec.ts",
    "format": "prettier '**/*.ts' --write",
    "test": "yarn clean && nyc --nycrc-path .nycrc.yaml mocha --reporter spec --exit 'test/**/*.spec.ts' 'test/*.spec.ts'",
    "test:integration": "yarn clean && nyc mocha 'integration/**/*.spec.ts' --reporter spec --exit",
    "prerelease": "yarn clean && yarn build",
    "release": "yarn prerelease && release-it",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-angular": "13.1.0",
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@nestjs/common": "7.6.17",
    "@nestjs/core": "7.6.17",
    "@nestjs/platform-express": "7.6.17",
    "@nestjs/swagger": "4.8.2",
    "@nestjs/testing": "7.6.15",
    "@release-it/conventional-changelog": "3.3.0",
    "@types/chai": "4.2.18",
    "@types/mocha": "9.0.0",
    "@types/node": "14.17.17",
    "@types/sinon": "10.0.3",
    "@types/sinon-chai": "3.2.5",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "ajv": "8.6.3",
    "chai": "4.3.4",
    "conventional-changelog-angular": "5.0.13",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.2",
    "light-my-request": "4.4.4",
    "lint-staged": "11.1.2",
    "mocha": "9.1.1",
    "nyc": "15.1.0",
    "pinst": "2.1.6",
    "prettier": "2.4.1",
    "reflect-metadata": "0.1.13",
    "release-it": "14.9.0",
    "rimraf": "3.0.2",
    "rxjs": "7.3.0",
    "semver": "7.3.5",
    "sinon": "11.1.2",
    "sinon-chai": "3.7.0",
    "source-map-support": "0.5.20",
    "supertest": "6.1.6",
    "ts-node": "10.2.1",
    "ts-toolbelt": "9.6.0",
    "tslib": "2.3.1",
    "typescript": "4.2.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0 || ^7.0.0",
    "@nestjs/core": "^6.0.0 || ^7.0.0",
    "@nestjs/swagger": "^4.7.15",
    "ajv": "^8.6.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0 || ^7.0.0"
  },
  "dependencies": {
    "fluent-json-schema": "3.0.1",
    "lodash.pickby": "4.6.0"
  }
}
