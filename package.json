{
  "name": "nestjs-validation",
  "version": "2.0.3",
  "description": "Validation module for NestJS application",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seedium/nestjs-validation.git"
  },
  "keywords": [
    "nestjs",
    "typescript",
    "seedium"
  ],
  "author": "Seedium <info@seedium.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seedium/nestjs-validation/issues"
  },
  "homepage": "https://github.com/seedium/nestjs-validation#readme",
  "scripts": {
    "clean": "rimraf -rf 'dist' 'tsconfig.tsbuildinfo'",
    "build": "yarn clean && tsc -p tsconfig.json",
    "watch": "yarn clean && tsc -w",
    "lint": "eslint lib/**/*.ts",
    "lint:test": "eslint -c .eslintrc.spec.yaml test/**/*.spec.ts",
    "format": "prettier '**/*.ts' --write",
    "test": "yarn clean && nyc --nycrc-path .nycrc.yaml mocha --reporter spec --exit 'test/**/*.spec.ts' 'test/*.spec.ts'",
    "test:integration": "yarn clean && nyc mocha 'integration/**/*.spec.ts' --reporter spec --exit",
    "prerelease": "yarn clean && yarn build",
    "release": "yarn prerelease && release-it",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-angular": "15.0.0",
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@nestjs/common": "8.2.4",
    "@nestjs/core": "8.2.4",
    "@nestjs/platform-express": "8.2.4",
    "@nestjs/swagger": "5.1.5",
    "@nestjs/testing": "8.2.4",
    "@release-it/conventional-changelog": "3.3.0",
    "@types/chai": "4.3.0",
    "@types/mocha": "9.0.0",
    "@types/node": "14.18.2",
    "@types/sinon": "10.0.6",
    "@types/sinon-chai": "3.2.6",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "ajv": "8.8.2",
    "chai": "4.3.4",
    "conventional-changelog-angular": "5.0.13",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.4",
    "light-my-request": "4.7.0",
    "lint-staged": "12.1.3",
    "mocha": "9.1.3",
    "nyc": "15.1.0",
    "pinst": "2.1.6",
    "prettier": "2.5.1",
    "reflect-metadata": "0.1.13",
    "release-it": "14.11.8",
    "rimraf": "3.0.2",
    "rxjs": "7.4.0",
    "semver": "7.3.5",
    "sinon": "12.0.1",
    "sinon-chai": "3.7.0",
    "source-map-support": "0.5.21",
    "supertest": "6.1.6",
    "ts-node": "10.4.0",
    "ts-toolbelt": "9.6.0",
    "tslib": "2.3.1",
    "typescript": "4.5.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "@nestjs/core": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "@nestjs/swagger": "^4.7.15 || ^5.0.0",
    "ajv": "^8.6.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0 || ^7.0.0"
  },
  "dependencies": {
    "fluent-json-schema": "3.0.1",
    "lodash.pickby": "4.6.0"
  }
}
